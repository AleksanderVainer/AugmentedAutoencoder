PTVERSION = 2
NAME = single_shot_multibox_detector 
MAINTAINER = Manuel Brucker <Manuel.Brucker@dlr.de>
DOCUMENTATION = http://www.google.com/search?q=tensorflow&btnI
LICENSE = Apache v2.0

SUPPORTED_LANGUAGES = Python
DEPENDS = sw.common.tensorflow==1.4, sw.common.py-tf-models-slim-git==2017.12.04, single_shot_multibox_detector_msgs==0
ARCHS = osl42-x86_64,
DESCRIPTION = Modified tensorflow SSD

EXEC_PATHS = ${ARCH}/bin
PYTHON_PATHS = ${ARCH}/lib/python2.7/site-packages

[subpackage]
NAME = opencv
DEPENDS += sw.common.OpenCV==3.1, sw.common.vtk==6.3.0

[subpackage]
NAME = ros
DEPENDS += sw.ros.indigo.desktop==1.1

[subpackage]
NAME = ln
DEPENDS = sw.common.links_and_nodes==0

[pre-install]
./build.sh

[install]
install/${ARCH} => ${ARCH}
build.sh => build.sh
ln_messages => ln_messages

[post-install]

